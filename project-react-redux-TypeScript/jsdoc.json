{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$|.js$",
    "excludePattern": "node_modules|documentation"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./documentation",
    "recurse": true,
    "verbose": true
  }
}
